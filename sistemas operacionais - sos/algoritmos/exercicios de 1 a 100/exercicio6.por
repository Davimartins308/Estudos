programa {
  funcao inicio() {
    real numero,antecessor,sucessor

    escreva("digite um numero\n")
    leia(numero)

    antecessor = numero-1
    sucessor = numero +1

    
    escreva("o antecessor de ",numero," é ",antecessor,"\n")
    escreva("o sucessor de ",numero," é ",sucessor,"\n")
    
  }
}
